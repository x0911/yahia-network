{
  "name": "yahia-network",
  "version": "1.0.0",
  "description": "A challenge with my friend about who can create a better light-weighted social network resulted in this app",
  "author": {
    "name": "Hamdi Mohamed"
  },
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "lets-go": "npm run build-spa && npm run replace-relative-paths ;",
    "build-spa": "nuxt build --spa",
    "replace-relative-paths": "replace '/nuxtfiles/' 'nuxtfiles/' dist --recursive && replace '/assets/' 'nuxtfiles/' dist --recursive && replace '(href|src|\"href\"|\"src\")=\"/' '$1=\"../' dist --recursive && replace '(href|src|\"href\"|\"src\"):\"/' '$1:\"../' dist --recursive",
    "resources": "capacitor-resources -p android"
  },
  "dependencies": {
    "@mdi/font": "^5.4.55",
    "@nuxt/content": "^1.11.1",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/firebase": "^7.5.0",
    "@nuxtjs/pwa": "^3.3.5",
    "age-calculator": "^1.0.0",
    "core-js": "^3.8.3",
    "device-detector-js": "^2.2.7",
    "firebase": "^8.2.10",
    "mobile-detect": "^1.4.4",
    "nuxt": "^2.14.12",
    "roboto-fontface": "^0.10.0",
    "typeface-cairo": "^1.1.13",
    "typeface-poppins": "^1.1.13",
    "v-mask": "^2.2.4",
    "vue-apexcharts": "^1.6.0",
    "vue-awesome-swiper": "^4.1.1",
    "vue-browser-detect-plugin": "^0.1.13",
    "vue-cookies": "^1.7.4",
    "vue-country-code": "^1.1.1",
    "vue-country-flag": "^2.0.3",
    "vue-countup-directive": "^1.2.3",
    "vue-custom-scrollbar": "^1.4.0",
    "vue-device-detector": "^1.1.6",
    "vue-inner-image-zoom": "^1.0.1",
    "vue-kinesis": "^1.2.3",
    "vue-localstorage": "^0.6.2",
    "vue-moment": "^4.1.0",
    "vue-qrcode-component": "^2.1.1",
    "vue-qrcode-reader": "^2.3.18",
    "vue-the-mask": "^0.11.1",
    "vue-typed-js": "^0.1.2",
    "vue-use-web": "^1.0.1",
    "vue-webrtc": "^1.2.2",
    "vue-window-size": "^1.0.3",
    "vue2-editor": "^2.10.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.11.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "prettier": "^2.2.1"
  }
}
